{
  "name": "H5pExtension",
  "author": "Niels Oldenburg",
  "url": "https://www.mediawiki.org/wiki/Extension:H5pExtension",
  "description": "h5pextension-desc",
  "version": "0.01",
  "license-name": "MIT",
  "type": "parserhook",
  "Hooks": {
    "ParserFirstCallInit": [
      "H5pExtension::onParserSetup"
    ]
  },
  "AutoloadClasses": {
    "H5pExtension": "H5pExtension.php",
    "H5p_Extension": "modules/H5p_Extension.php",
    "H5pMediawiki":"modules/H5pMediawiki.php",
    "H5pEventClass":"modules/H5pEventClass.php",
    "H5PCore": "h5p-php-library/h5p.classes.php",
    "H5PFrameworkInterface": "h5p-php-library/h5p.classes.php",
    "H5PContentValidator" : "h5p-php-library/h5p.classes.php",
    "H5PValidator" : "h5p-php-library/h5p.classes.php",
    "H5PStorage" : "h5p-php-library/h5p.classes.php",
    "H5PExport" : "h5p-php-library/h5p.classes.php",
    "H5PDevelopment" : "h5p-php-library/h5p-development.class.php",
    "H5PFileStorage" : "h5p-php-library/h5p-file-storage.interface.php",
    "H5PDefaultStorage" : "h5p-php-library/h5p-default-storage.class.php",
    "H5PEventBase" : "h5p-php-library/h5p-event-base.class.php",
    "H5peditor" : "h5p-editor-php-library/h5peditor.class.php",
    "H5peditorFile" : "h5p-editor-php-library/h5peditor-file.class.php",
    "H5peditorStorage" : "h5p-editor-php-library/h5peditor-storage.interface.php",
    "H5PEditorAjaxInterface" : "h5p-editor-php-library/h5peditor-ajax.interface.php",
    "H5PEditorAjax" : "h5p-editor-php-library/h5peditor-ajax.class.php",
    "H5pExtensionMediawikiEditorStorage" : "Editor/H5pExtensionMediawikiEditorStorage.php",
    "H5pExtensionMediawikiEditorAjax" : "Editor/H5pExtensionMediawikiEditorAjax.php",
    "SpecialH5pExtension":"SpecialH5pExtension_body.php"
  },
  "SpecialPages" :{
    "H5pExtension":"SpecialH5pExtension"
  },
  "manifest_version": 1,
  "MessageDirs": {
    "H5pExtension": [
      "i18n"
    ]
  },
  "ResourceModules": {
    "ext.H5pExtension.editor": {
      "scripts": [
        "h5p-editor-php-library/scripts/h5p-hub-client.js",
        "h5p-editor-php-library/scripts/h5peditor-selector-hub.js",
        "h5p-editor-php-library/scripts/h5peditor.js",
        "h5p-editor-php-library/scripts/h5peditor-editor.js",
        "h5p-php-library/js/jquery.js",
        "h5p-php-library/js/h5p.js",
        "h5p-php-library/js/h5p-event-dispatcher.js",
        "h5p-php-library/js/h5p-x-api.js",
        "h5p-php-library/js/h5p-x-api-event.js",
        "h5p-php-library/js/h5p-content-type.js",
        "h5p-php-library/js/h5p-confirmation-dialog.js",
        "h5p-php-library/js/h5p-action-bar.js",
        "h5p-php-library/js/h5p-utils.js",
        "h5p-php-library/js/h5p-data-view.js",
        "Editor/scripts/h5p-editor.js",
        "h5p-php-library/js/h5p-version.js",
        "h5p-php-library/js/h5p-version.js",
        "h5p-php-library/js/h5p-embed.js",
        "h5p-php-library/js/h5p-utils.js",
        "h5p-php-library/js/h5p-display-options.js"
      ],
      "styles": [
        "h5p-editor-php-library/styles/css/application.css",
        "h5p-editor-php-library/styles/css/fonts.css",
        "h5p-editor-php-library/styles/css/h5p-hub-client.css"
      ],
      "messages": [
        "h5p-extension-label"
      ],
      "dependencies": [
        "jquery.cookie",
        "jquery.tabIndex"
      ]
    },
    "ext.H5pExtension.player":{
      "styles":[
        "h5p-php-library/styles/h5p.css"
      ]
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "",
    "remoteExtPath": "H5pExtension"
  }
}
